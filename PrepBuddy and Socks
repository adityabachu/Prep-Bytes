import java.util.*;
import java.io.*;

public class Main {
  public static void main(String args[]) throws IOException {
    
    //write your code here
    Scanner sc = new Scanner(System.in);
    int t = sc.nextInt();
    for(int i = 0; i < t; i++)
    {
      int n = sc.nextInt();
      if(n==1)
      {
        System.out.println("2");
      }
      else
      {
        System.out.println(n+1);
      }
    }
    
  }
}
